{
 "Titan":{
   "UseConsole": true,
   "UseElk": true,
   "DefaultLevel": "debug",
   "LogLevelRestriction": "information",
   "Elk": {
     "Url": "http://localhost:9200",
     "IndexFormat": "circa-actors" 
   }
 },
  "Callisto":{
    "Database": "circa-actors",
    "Servers": ["localhost:27017"]
  },
  "Deimos":{
   "TracingEnabled": true,
    "UseCorrelationId": true,
    "Jaeger":{
      "Enabled": true,
      "ServiceName": "circa-actors",
      "UdpHost": "127.0.0.1"
    }
  }
}
